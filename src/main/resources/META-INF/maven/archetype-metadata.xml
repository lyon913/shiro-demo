<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor name="spring-webapp">
    <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
            <directory>src</directory>
            <includes>
                <include>**/*</include>
            </includes>
            <excludes>
                <exclude>src/main/resources/META-INF/maven/*</exclude>
            </excludes>
        </fileSet>
    </fileSets>
</archetype-descriptor>
